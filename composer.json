{
    "name": "24hoursmedia-craftcms/twig-works-image",
    "description": "Twig image transforms for CraftCMS 3.",
    "type": "craft-plugin",
    "version": "1.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "twigworksimage",
        "twig",
        "image"
    ],
    "support": {
        "docs": "https://github.com/24hoursmedia-craftcms/twig-works-image/blob/master/README.md",
        "issues": "https://github.com/24hoursmedia-craftcms/twig-works-image/issues"
    },
    "license": "proprietary",
    "authors": [
        {
            "name": "24HOURSMEDIA",
            "homepage": "http://www.24hoursmedia.com"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0-RC1"
    },
    "autoload": {
        "psr-4": {
          "twentyfourhoursmedia\\twigworksimage\\": "src/"
        }
    },
    "extra": {
        "name": "TwigWorksImage",
        "handle": "twig-works-image",
        "hasCpSettings": true,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/t4hoursmedia-craftcms/twig-works-image/master/CHANGELOG.md",
        "components": {
            "twigWorksImageService": "twentyfourhoursmedia\\twigworksimage\\services\\TwigWorksImageService"
        },
        "class": "twentyfourhoursmedia\\twigworksimage\\TwigWorksImage"
    }
}
